

       @{

           FormsIdentity id = (FormsIdentity)User.Identity;
           FormsAuthenticationTicket ticket = id.Ticket;
       }


    }
    private void Signout_Click(Object sender, EventArgs e)
    {
        FormsAuthentication.SignOut();
        Response.Write("Logged out - cookie deleted.");
    }

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Forms Authentication</title>
</head>
<body>
    <h3>Forms Authentication Example</h3>
    <span> Welcome! @Html.Encode(User.Identity.Name)</span>
    <form>
        <input type="submit" value="Signout" runat="server" onserverclick="Signout_Click" />
        <h3>Forms Authentication Ticket Properties</h3>
        <table>
            <tbody>
                <tr>
                    <td>
                        CookiePath: @ticket.CookiePath
                    </td>
                    <td>
                        <asp:Label id="cookiePath" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        Expiration:@ticket.Expiration.ToString()
                    </td>
                    <td>
                        <asp:Label id="expireDate" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        Expired:@ticket.Expired.ToString()
                    </td>
                    <td>
                        <asp:Label id="expired" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        IsPersistent:@ticket.IsPersistent.ToString()
                    </td>
                    <td>
                        <asp:Label id="isPersistent" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        IssueDate:@ticket.IssueDate.ToString();
                    </td>
                    <td>
                        <asp:Label id="issueDate" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        Name:@ticket.Name
                    </td>
                    <td>
                        <asp:Label id="name" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        UserData:@id.Ticket.UserData
                    </td>
                    <td>
                        <asp:Label id="userData" runat="server" />
                    </td>
                </tr>
                <tr>
                    <td>
                        Version:@ticket.Version.ToString()
                    </td>
                    <td>
                        <asp:Label id="version" runat="server" />
                    </td>
                </tr>
            </tbody>
        </table>
        <p>
        </p>
    </form>
</body>
</html>